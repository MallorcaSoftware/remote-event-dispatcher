{
    "name": "wildpascal/remote-event-dispatcher",
    "description": "Remote event dispatcher",
    "authors": [
        {
            "name": "Pascal Wild",
            "email": "pallidotcom@gmail.com"
        }
    ],
    "suggest": {
        "php-amqplib/php-amqplib": "Allow sending log messages to an AMQP server using php-amqplib"
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/event-dispatcher": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "escapestudios/symfony2-coding-standard": "~2.0",
        "squizlabs/php_codesniffer": "2.*",
        "phing/phing": "2.*",
        "phpmd/phpmd": "@stable",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*",
        "mayflower/php-codebrowser": "~1.1",
        "fzaninotto/faker": "^1.6@dev",
        "php-amqplib/php-amqplib": "~2.4",
        "ext-gearman": "*"
    },
    "autoload": {
        "psr-4": { "WP\\RemoteEventDispatcher\\": "" }
    }
}
